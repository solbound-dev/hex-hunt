{
  "name": "hex-hunt",
  "devDependencies": {
    "turbo": "^2.5.5"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "start": "node apps/api/dist/main",
    "db:generate": "yarn workspace api run db:generate",
    "db:migrate": "yarn workspace api run db:migrate",
    "db:studio": "yarn workspace api run db:studio",
    "db:seed": "yarn workspace api run db:seed",
    "prisma": "yarn workspace api run prisma"
  },
  "packageManager": "yarn@3.6.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "license": "none",
  "private": true
}
